<ion-view>
	<ion-content scroll="false" class="has-header padding  aiv-pos-back" style="padding:0px;">
		<!-- TAKEAWAY VIEW -->
		<div ng-show="posview.type=='POS'" class="row" style="height:100%;padding:0px;">
			<div class="col col-40 flex-container" style="background-color:#FFF;padding:0px 5px;">
				<!--<div data-barcode-scanner="barcodeScanned" data-ng-model="aiv_scan_input"></div>-->
				<div ng-if="hf_loyalty.enabled" data-barcode-scanner="searchLoyaltyCustomer" data-ng-model="loyalty_card_id"></div>
				<label class="item item-input flex-item" style="border:none;">
					<i class="icon ion-search placeholder-icon"></i>
					<input id="aiv-scan-input" ng-change="barcodeScanned(aiv_scan_input,false,false)" ng-click="aiv_scan_input=''" type="text" placeholder="Product Search/Barcode" ng-model="aiv_scan_input" ng-virtual-keyboard="kb_config" style="height:19px;"/>
				</label>
				<div class="row header responsive-sm flex-item" style="padding:0 5px;background-color:#387ef5;color:#FFF;">
					<div class="col col-50 ">Name</div>
					<div class="col col-center">Qty</div>
					<div class="col col-center">Price</div>
					<div class="col col-center">Total</div>
					<div class="col col-10"></div>
				</div>
				<ion-scroll id="cart-scroll" class="flex-item flex-grow" direction="y" delegate-handle="cartScroll">
			        <ion-list>
						<div ng-repeat="product in cartItems track by $index" ng-init="productIndex = $index">
						<ion-item class="item item-text-wrap pos-cart pos-list"  ng-init="calculateTotal(product,$index)" ng-click="checkSetSelectedItem($index,product);" ng-class="{selected : $index === idSelectedItem.index,'aiv-disabled':compulsory_option_mode}">
							<div class="row responsive-sm product-gallery cart-list">
								<div  class="col col-50 col-center"  style="font-size: 0.8em;font-weight:bold;">{{product.title}}</div>
								<div class="col col-10 col-center" style="font-size: 0.8em;font-weight:bold;">{{product.quantity}}</div>
								<div class="col col-15 col-center " style="font-size: 0.8em;font-weight:bold;">{{product.sel_variations.attributes?product.sel_variations.price:product.price | currency:"":2}}</div>
								<div class="col col-15 col-center " style="font-size: 0.8em;font-weight:bold;">{{product.sel_variations.attributes?product.sel_variations.price*product.quantity:product.price*product.quantity | currency:"":2}}</div>
								<div class="col col-10 col-center text-right">
									<!--<button class="button button-small calm " ng-click="openNoteModal('PRODUCT',product)"><i class="icon ion-clipboard"></i></button>-->
									<button ng-disabled="!product.edit" class="button button-small assertive" ng-click="deleteProduct($index);"><i class="icon ion-close"></i></button>
								</div>
							</div>
						</ion-item>	
						<ion-item ng-repeat="(key, value) in product.sel_variations.attributestoshow track by $index" ng-init="optionIndex = $index" class="item item-text-wrap pos-cart pos-list" ng-click="setSelectedSubItem(productIndex,optionIndex,product,key)" ng-class="{'option-selected' : $index === idSelectedSubItem.index&&productIndex === idSelectedItem.index,'aiv-disabled':compulsory_option_mode}">
							<div class="row responsive-sm product-gallery cart-list"  >
								<div  class="col col-50 col-center"  style="color:#F00;font-size: 0.8em;font-weight:bold;">--{{key| split:'###':0 | split:'_':0 }}</div>
								<div class="col col-10 col-center" style="font-size: 0.8em;font-weight:bold;">1</div>
								<div class="col col-15 col-center " style="font-size: 0.8em;font-weight:bold;">{{value.toFixed(2)}}</div>
								<div class="col col-15 col-center " style="font-size: 0.8em;font-weight:bold;"></div>
								<div class="col col-10 col-center text-right"><!--<button class="button button-small calm " ng-click="editOptions(product)" ng-if="product.sel_variations.attributes" ><i class="icon ion-edit"></i></button>-->
									<button ng-disabled="!product.edit||productIndex!=cartItems.length-1" class="button button-small assertive" ng-click="deleteSub(key,product);"><i class="icon ion-close"></i></button>
								</div>
							</div>
						</ion-item>
						<ion-item ng-show="product.dash" class="item item-text-wrap pos-cart pos-list">
							<div class="row responsive-sm product-gallery cart-list">
								<div class="col col-50 col-center"  style="font-weight:bold;">----------------</div>
								<div class="col col-center" style="font-weight:bold;"></div>
								<div class="col col-center " style="font-weight:bold;"></div>
								<div class="col col-center " style="font-weight:bold;"></div>
								<div class="col col-20 col-center text-right">
									<button ng-disabled="!product.edit" class="button button-small assertive" ng-click="updateDash(false,productIndex);"><i class="icon ion-close"></i></button>
								</div>
							</div>
						</ion-item>
						</div>
					</ion-list>
				</ion-scroll>
				<!--<div ng-if="terminalData.type!='FRONT' && !cartItems.length" class="flex-item pos-cart">
					<button class="button button-positive button-block" ng-click="openShippingModal();"><i class="icon ion-plus-circled"></i> New Order</button>
				</div>-->
				<div class="flex-item pos-cart" style="border: 1px solid #dddddd;border-right:0px;border-left:0px;background-color: #ffc900;">
					<!--<div ng-if="DealArr['B1G1']['show_msg']" class="item item-button-right item-assertive" style="border: 0px;">
						<strong>{{DealArr['B1G1']['msg']}}</strong>
						<button class="button button-small button-positive" ng-click="getDealDiscount(true)">APPLY</button>
					</div>-->
					<div ng-if="deal_status.show_msg&&!deal_status.status" class="item item-assertive text-center" style="border: 0px;padding: 5px;">
						<strong>{{deal_status.msg}}</strong>
					</div>
					<div ng-if="formCheckout.shipping_note" class="item" style="border:0px;padding:5px;color:#999;">
						<strong  >Note:</strong> {{formCheckout.shipping_note}}
					</div>
					<div class="row responsive-sm"  >
						<div class="col col-25 col-bottom">
							<button ng-if="startup.action=='TABLEVIEW'" class="button button-full button-balanced " title="table number" ><span class=""> TABLE {{selectedTable.number}}</span></button>
							<button ng-if="DealArr['B1G1']['show_msg']" class="button button-full button-assertive" ng-click="applyOffer();"><strong>{{DealArr['B1G1']['msg']}}?</strong></button>
							<button ng-disabled="loginInfo.role=='user'" class="button button-full button-royal icon ion-folder" title="Open Cashdrawer" ng-click="openCashdrawer();"><span class="bar-btn-text"> Drawer</span></button>
						</div>
						<div class="col col-center text-right">
							<div class="row responsive-sm" style="margin-top:5px;" >
								<div class="col col-35 col-center text-left aiv-text" style="font-weight:bolder;color:#333;">Sub Total</div>
								<div class="col col-center text-center aiv-text">{{grand.Total | currency:"£":2}}</div>
							</div>
							<div class="row responsive-sm aiv-text" style="margin-top:0px;">
								<div class="col col-35 col-center text-left aiv-text" style="font-weight:bolder;color:#333;">Discount</div>
								<div class="col col-center text-center"><button style="width:100%" class="button button-small button-positive aiv-text" ng-class="grand.Discount?'button-assertive':'button-light'" ng-click="openDiscountModal();">-{{grand.Discount | currency:"£":2}}</button></div>
							</div>
							<div class="row responsive-sm aiv-text" style="margin-top:0px;" ng-if="startup.action!='TABLEVIEW'">
								<div class="col col-35 col-center text-left aiv-text" style="font-weight:bolder;color:#333;" >Delivery</div>
								<div class="col col-center text-center aiv-text"  ><button style="width:100%" class="button button-small aiv-text" ng-class="formCheckout.shipping_lines[0].method_id == 'local_delivery'?'button-assertive':'button-positive'" ng-click="openShippingModal();">{{formCheckout.shipping_lines[0].total | currency:"£":2}}</button></div>
							</div>
							<div class="row responsive-sm aiv-text" style="margin-top:0px;" ng-if="startup.action!='TABLEVIEW'">
								<div class="col col-35 col-center text-left aiv-text aiv-text-wrap-off" style="font-weight:bolder;color:#333;" >Bag Charge</div>
								<div class="col col-center text-center aiv-text"  ><button style="width:100%" class="button button-small button-positive aiv-text" ng-click="openBagChargesPopup();">{{bagCharges.selected | currency:"£":2}}</button></div>
							</div>
							<div class="row responsive-sm" >
								<div class="col col-35 col-center h3 text-left" style="font-weight:bolder;" >Total</div>
								<div class="col col-center h3 text-center" style="font-weight:bolder;" >{{grand.Total + grand.Shipping - grand.Discount + bagCharges.selected | currency:"£":2}}</div>
							</div>
						</div>
					</div>
				</div>
				<div class="flex-item main-buttons">
					<div class="button-bar pos-buttons1">
						<label class="item item-input button-large" style="border:0px;">
							<input ng-disabled="!idSelectedItem.item.edit" class="qty-txt" ng-model="pos_input.quantity" type="text" placeholder="Qty" ng-change="changeQuantity()"/>
						</label>
						<button class="button button-large button-positive  icon ion-plus" ng-class="{'disabled':idSelectedItem.item==null}" ng-click="addQuantity(idSelectedItem.item);" title="Increase qty"></button>
						<button ng-disabled="!idSelectedItem.item.edit" class="button button-large button-positive  icon ion-minus" ng-class="{'disabled':idSelectedItem.item==null}" ng-click="subtractQuantity(idSelectedItem.item,idSelectedItem.index);" title="Decrease qty"></button>
						<button class="button button-large button-positive icon ion-arrow-up-a " ng-class="{'disabled':idSelectedItem.item==null}" ng-click="cartMove('UP')" title="UP"></button>
						<button class="button button-large button-positive  icon ion-arrow-down-a " ng-class="{'disabled':idSelectedItem.item==null}" ng-click="cartMove('DOWN')" title="DOWN"></button>
					</div>
					<div class="row">
						<div class="col col-66">
							<div class="button-bar bar-royal pos-buttons2 flex-row">
								<button ng-if="startup.action!='TABLEVIEW'" class="button button-large icon ion-bookmark" title="Save order" ng-click="complete('pending');"><span class="bar-btn-text"> Save</span></button>
								<button ng-if="startup.action=='TABLEVIEW'" class="button button-large icon ion-arrow-left-a" title="Table View" ng-click="backToTableView();"><span class="bar-btn-text"> Table View</span></button>
								<button class="button button-large icon ion-clipboard" ng-click="openNoteModal('ORDER');" title="Add note"><span class="bar-btn-text"> Note</span></button>
							</div>
							<div class="button-bar pos-buttons3 flex-row">
								<button class="button button-large button-assertive icon ion-trash-a" ng-if="startup.action=='TABLEVIEW'" ng-click="backToTableView();" title="Clear Order"><span class="bar-btn-text"> Clear Order</span></button>
								<button class="button button-large button-assertive icon ion-trash-a" ng-if="startup.action!='TABLEVIEW'"ng-click="clearOrder();" title="Clear Order"><span class="bar-btn-text"> Clear Order</span></button>
								<button class="button button-large button-positive icon ion-bag" ng-click="openItemModal();" title="Open item"><span class="bar-btn-text"> Open Item</span></button>
							</div>
						</div>
						<div class="col col-33">
							<div class="button-bar pay-button flex-row">
								<button ng-if="startup.action=='TABLEVIEW'" class="button button-large button-balanced icon ion-checkmark" title="Place order" ng-click="placeTableOrder('on-hold');"><span class="bar-btn-text"> Place Order</span></button>
								<button ng-if="startup.action!='TABLEVIEW'" class="button button-large button-balanced icon ion-cash" ng-click="openPaymentModal();" title="Payment"><span class="bar-btn-text"> Payment</span></button>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="col back-image" style="padding:0px;">
				<ion-scroll direction="y" style="height:calc(100vh - 150px);" lazy-scroll delegate-handle="productScroll">
					<div class="aiv-spinner"><ion-spinner ng-show="product_search_start" icon="android"></ion-spinner></div>
					<div ng-if="sub_categories.items.length&&!sub_categories.show_as_product">
						<div class="row responsive-sm product-gallery">
							<div class="col col-20 aiv-click zero-padding" ng-class="{'aiv-disabled':compulsory_option_mode}" ng-repeat="sub in sub_categories.items | orderBy:'menu_order' track by sub.id" ng-click="onClickSub(sub);">
								<div class="item product-style" style="min-height:50px;" ng-class="{'prdt-selected' : sub_categories.selected == sub.slug}">
									<label class="item-text-wrap h5 product-letter" >{{sub.name | decodehtml}}</label>
								</div>
							</div>
						</div>
						<hr style="border: 1px solid #ddd;">
					</div>
					<label class="item item-text-wrap h5 text-center prdt-instruction" ng-if="sel_product.meta.wc_productdata_options[0].instruction&&!product_show">{{sel_product.meta.wc_productdata_options[0].instruction}}</label>
					<label class="item item-text-wrap h5 prdt-instruction" style="background-color:#387ef5;" ng-if="!product_show&&otheroptions!=null&&otheroptions.length>0&&!sub_categories.subcategory_mode">Select {{otheroptions[0].parent.name}}</label>
					<div class="row responsive-sm product-gallery ">
						<div class="col col-33" ng-if="edit_mode">
							<button class="button button-block button-positive button-outline icon ion-plus" ng-click="openUpdateProduct();" title="Add product"></button>
						</div>
						<div ng-if="product_show" class="col col-25 aiv-click zero-padding" ng-repeat="product in products | orderBy:'menu_order' track by product.id" ng-click="onClickProduct(product)">
							<div class="item product-style" ng-class="{'prdt-selected':set_product_color(product),'padding-top':edit_mode}" ng-style="{'background-color':product.color}">
								<label class="item-text-wrap h5 product-letter" >{{product.title| limitTo: 60}}</label>
								<div class="note product-note">
									<span ng-if="product_search" style="float:left;">{{product.categories[0] | decodehtml | split:'###':0}}</span>
									<!--<span ng-if="product.meta.wc_productdata_options[0]._dealcode" class="aiv-offer-label">{{::product.meta.wc_productdata_options[0]._dealcode}}</span>-->
								</div>
								<button ng-if="edit_mode" class="button button-small button-outline button-positive icon item-update" ng-class="product_edited[$index]?'ion-checkmark':'ion-edit'" ng-click="openUpdateProduct(product,$index);$event.stopPropagation();" title="Edit product"></button>
								<button ng-if="edit_mode" class="button button-small button-outline button-assertive icon item-delete" ng-class="product_removed[$index]?'ion-checkmark':'ion-close'" ng-click="removeProduct(product,$index);$event.stopPropagation();" title="Remove product"></button>
							</div>
						</div>
						<div ng-if="!product_show" class="col col-25 aiv-click" style="padding:0px;" ng-repeat="product in otheroptions track by $index" ng-click="onClickOthers(product)">
							<div class="item product-style" ng-style="{'background-color':product.color}">
								<label class="item-text-wrap h5 product-letter" >{{product.stitle| limitTo: 60}}</label>
							</div>
						</div>
						<div ng-if="!product_show&&otheroptions!=null&&otheroptions.length>0&&otheroptions.next.length" class="col col-25 aiv-click" style="padding:0px;">
							<div  class="item product-style balanced" style="background-color:#33cd5f">
								<button class="button button-large button-balanced button-full icon ion-arrow-right-a" ng-disabled="compulsory_option_mode" title="Next" ng-click="nextSub(otheroptions.next!=null&&otheroptions.next.length>0)"></button>
							</div>
						</div>
						<div ng-if="!product_show&&(otheroptions.next==null||otheroptions.next.length==0)&&!sub_categories.subcategory_mode" class="col col-25 aiv-click" style="padding:0px;"  ng-click="exitSub(true)">
							<div  class="item product-style" style="background-color:#76FF03;color:#111">
								<label ng-class="{'aiv-disabled':compulsory_option_mode}" class="item-text-wrap h5 product-letter" style="color: #111!important;font-weight: bold;" >Finish</label>
							</div>
						</div>
					</div>
				</ion-scroll>
				<!--<div class="row footer-block" >
					<div class="col" style="padding:0;" >
						<button class="button button-large button-full icon ion-arrow-left-a button-calm " title="Previous" ng-click="prevSub(otheroptions.prev!=null&&otheroptions.prev.length>0)"></button>
					</div>
					<div class="col" style="padding:0;" >
						<button class="button button-large button-full icon ion-arrow-right-a button-positive" ng-disabled="compulsory_option_mode" title="Next" ng-click="nextSub(otheroptions.next!=null&&otheroptions.next.length>0)"></button>
					</div>
					<div class="col col-15" style="padding:0;" >
						<button style="padding:0px" class="button button-large button-full button-balanced" ng-disabled="compulsory_option_mode" title="Done" ng-click="exitSub(true);">DONE</button>
					</div>
					<div class="col col-25" style="padding:0;" >
						<button class="button button-large button-full button-assertive " title="Cooking Instructions" ng-disabled="compulsory_option_mode" ng-click="addInstructions();">Cook Inst</button>
					</div>
					<div class="col" style="padding:0;" >
						<button style="padding:0px;" class="button button-large button-full button-royal " title="Extras/Toppings" ng-disabled="compulsory_option_mode" ng-click="editOptions(idSelectedItem.item);">Options</button>
					</div>
				</div>-->
				<div class="row footer-block" >
					<div class="col" style="padding:0;" >
						<button class="button  button-full icon ion-arrow-left-a button-calm " title="Previous" ng-click="prevSub(otheroptions.prev!=null&&otheroptions.prev.length>0)"></button>
					</div>
					<div class="col" style="padding:0;" >
						<button style="padding:0px" class="button  button-full button-balanced" ng-disabled="compulsory_option_mode" title="Done" ng-click="exitSub(true);">DONE</button>
					</div>
					<div class="col col" style="padding:0;" >
						<button class="button   button-full button-creative" title="Dash" ng-click="updateDash(true);">------</button>
					</div>
					<div class="col col-15" style="padding:0;" >
						<button class="button   button-full  button-assertive " title="Extra" ng-click="ExtraClick('Extra');">Extra</button>
					</div>
					<div class="col col-15" style="padding:0;" >
						<button class="button button-full button-calm " title="No prefix" ng-click="noClick();">No</button>
					</div>
					<div class="col" style="padding:0;" >
						<button style="padding:0px;" class="button   button-full button-royal " title="Extras/Toppings" ng-click="editOptions(idSelectedItem.item);">Options</button>
					</div>
				</div>
				<div class="row footer-block" >
					<div class="col" style="padding:0;" >
						<button class="button  button-full button-balanced " title="Cooking Instructions" ng-click="addInstructions();">Cook Inst</button>
					</div>
					<div class="col" style="padding:0;" >
						<button class="button   button-full button-royal " title="On" ng-click="footerClick('On');">On</button>
					</div>
					<div class="col col-20" style="padding:0;" >
						<button class="button  button-full button-calm" title="1st Half" ng-click="footerClick('1Half');">1Half</button>
					</div>
					<div class="col col-20" style="padding:0;" >
						<button class="button   button-full button-positive" title="2nd Half" ng-click="footerClick('2Half');">2Half</button>
					</div>
					<div class="col" style="padding:0;" >
						<button class="button   button-full button-positive" title="All Over" ng-click="footerClick('All Over');">All Over</button>
					</div>
				</div>
			</div>
			<div style="padding:0px;width:130px;">
				<ion-scroll direction="y" class="wide-as-needed menu-back-design aiv-pos-back" delegate-handle="categoryScroll" style="height:calc(100vh - 44px);">
					<div class="list flex-container">
						<ion-item ng-if="item.slug.indexOf('open-item1')<0&&!item.parent&&((startup.action=='TABLEVIEW'&&(item.serve_type.includes('sitin')||item.serve_type=='both'))||(startup.action!='TABLEVIEW'&&(item.serve_type==''||item.serve_type=='both'||item.serve_type.split(',').indexOf('takeaway')>-1)))" ng-class="{'menu-selected' : sel_category == item.slug,even: !item.odd}" class="menu-design item-text-wrap side-menu flex-child" ng-repeat="item in categories track by item.id" ng-click="getProducts(item,true)">
							{{::item.name | decodehtml}}
						</ion-item>	
					</div>
				</ion-scroll>
			</div>
		</div>
		<!-- TABLE VIEW -->
		<div ng-show="posview.type=='TABLE'" class="row" style="height:100%;">
			<div class="col col-25 " style="background-color:#FFF">
				<div class="row" style="padding-top:0;padding-bottom:0;">
					<div class="col flex-container" style="height: calc(100vh - 100px);max-height: calc(100vh - 100px);padding-top:0;padding-bottom:0;">
						<div class="row">
							<div class="col col-50 col-center text-center">
								<button class="button button-small  button-stable button_circle" ></button>
								<p class="text_font">Empty</p>
							</div>
							<div class="col col-50 col-center text-center">
								<button class="button button-small  button-calm button_circle" ></button>
								<p class="text_font">Mains Released</p>
							</div>
						</div>
						<div class="row">
							<div class="col col-50 col-center text-center">
								<button class="button button-small  button-energized button_circle" ></button>
								<p class="text_font">No Food Ordered</p>
							</div>
							<div class="col col-50 col-center text-center">
								<button class="button button-small  button-positive button_circle" ></button>
								<p class="text_font">Afters Ordered</p>
							</div>
						</div>
						<div class="row">
							<div class="col col-50 col-center text-center">
								<button class="button button-small  button_circle"  style="background-color: #FF9800"></button>
								<p class="text_font">Starter Released</p>
							</div>
							
							<div class="col col-50 col-center text-center">
								<button class="button button-small button_circle" style="background-color: #009688"></button>
								<p class="text_font">Drinks Released</p>
							</div>
						</div>
						<div class="row">
							<div class="col col-50 col-center text-center">
								<button class="button button-small  button-balanced button_circle" ></button>
								<p class="text_font">Food Ordered</p>
							</div>
							<div class="col col-50 col-center text-center">
								<button class="button button-small  button-assertive button_circle" ></button>
								<p class="text_font">Billed</p>
							</div>
						</div>
						<div class ="row side_menu_BUTTON side_menu" ng-show="selectedTable.number">
							<div style="padding:0px;" class="col col-20 col-center text-center side_menu_BUTTON">
								<label class="item positive side_menu_BUTTON font_side">{{selectedTable.number}}</label>
							</div>
							<div style="padding:0px;" class="col col-15 col-center text-center side_menu_BUTTON">
								<label class="item positive side_menu_BUTTON font_side">{{selectedTable.cover}}</label>
							</div>
							<div style="padding:0px;" class="col col-50 col-center text-center side_menu_BUTTON">
